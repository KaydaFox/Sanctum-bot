{
	"name": "sanctum_bot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "KaydaFox",
	"license": "UNLICENSE",
	"type": "module",
	"imports": {
		"#root/*": [
			"./dist/*.js"
		],
		"#lib/*": [
			"./dist/lib/*.js"
		]
	},
	"dependencies": {
		"@androz2091/discord-invites-tracker": "^1.2.3",
		"@prisma/client": "^5.21.1",
		"@sapphire/decorators": "^6.1.0",
		"@sapphire/discord.js-utilities": "^7.3.0",
		"@sapphire/duration": "^1.1.2",
		"@sapphire/eslint-plugin-result": "^2.0.4",
		"@sapphire/framework": "^5.2.1",
		"@sapphire/plugin-hmr": "^3.0.1",
		"@sapphire/plugin-logger": "^4.0.2",
		"@sapphire/plugin-subcommands": "^6.0.3",
		"@sapphire/plugin-utilities-store": "^2.0.2",
		"@sapphire/utilities": "^3.17.0",
		"@skyra/env-utilities": "^1.3.0",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"colorette": "^2.0.20",
		"discord.js": "^14.16.3",
		"lint-staged": "^15.2.10",
		"pretty-quick": "^4.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@kaydafox/typescript-config": "^1.0.0",
		"@sapphire/cli": "^1.9.3",
		"@sapphire/eslint-config": "^5.0.5",
		"@sapphire/prettier-config": "^2.0.0",
		"@types/eslint": "^8.56.12",
		"@types/node": "^20.17.0",
		"@types/ws": "^8.5.12",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.6",
		"prettier": "^3.3.3",
		"prisma": "^5.21.1",
		"tsc-watch": "^6.2.0",
		"typescript": "^5.4.5"
	},
	"scripts": {
		"postinstall": "prisma generate",
		"sapphire": "sapphire",
		"generate": "sapphire generate",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"start": "node dist/index.js",
		"dev": "npm run build && npm run start",
		"watch:start": "tsc-watch -b src --onSuccess \"npm run start\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@4.5.1"
}
